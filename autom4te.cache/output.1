do
  IFS=$as_save_IFS
  case $as_dir in #(((
    '') as_dir=./ ;;
    */) ;;
    *) as_dir=$as_dir/ ;;
  esac
  as_found=:
  
  printf "%s\n" "$as_me:${as_lineno-$LINENO}:  trying $as_dir" >&5
